    <div class="row p-2">
      <% for(var i = 0; i < blogs.length; i++){%>
      <div style="box-shadow: 0 5px 20px 0 rgba(0,0,0,.2);
      border-radius: 18px;" class="p-0 pb-2 mb-2 d-flex align-items-stretch col-md-4 bg-gray">
        <div class="blog-post mb-0">
          <div class="image"><img style="width:100%!important;" src="<%=blogs[i]['image']%>" alt="<%=blogs[i]['title']%>">
            <div class="overlay d-flex align-items-center justify-content-center"><a href="/blog/<%=blogs[i]['blogurl']%>" class="btn btn-outline-light">Read More</a></div>
          </div>
          <!-- <div class="author"><img src="img/avatar.jpg" alt="author" class="img-fluid"></div> -->
          <div class="text bg-gray"><a href="/blog/<%=blogs[i]['blogurl']%>">
              <h3 style="color: cornflowerblue;" class="text-this"><%=blogs[i]['title']%></h3></a>
            <ul class="post-meta list-inline">
              <li class="list-inline-item"><i class="icon-clock-1"></i> <%=moment(blogs[i]["date"]).format("DD/MMM/YYYY HH:mm A")%></li>
              <li class="list-inline-item"><i class="icon-chat"></i>by <%=blogs[i]['author']%></li>
            </ul>
            <!-- <div class="d-flex align-items-center justify-content-between mb-1">
