                                <small class="text-danger phoneHelp d-none">
                                </small>  
                            </div>
                            <div class="form-group formField">
                                <input name="password" required="" type="password" class="form-control password" placeholder="Password">
                                <small class="text-danger passwordHelp d-none">
                                </small>  
                            </div>
                            <input name="auth" class="auth" type="hidden" value="true">
                            <div class="form-group formField">
