    <!-- Hero Section-->
    <section class="hero hero-page mt-3">
        <div class="container p-2">
            <div class="row">
                <div class="col-md-2">

                </div>
                <div class="col-md-8">
                    <h1 class="text-center">Post a Job or an Internship</h1>
                    <h5 class="text-center mb-4" style="color: grey">Post your job and internship opportunities and receive applications from students and marketers looking out for a job</h5>
                </div>
            </div>
            <div class="row">
                <div class="col-12">
                    <div class="tab-content row justify-content-center">
                        <div class="tab-pane container active p-2 col-md-8" style="border: 1px solid lightgrey;     box-shadow: 0 5px 20px 0 rgba(0,0,0,.2);
                        border-radius: 18px; background: white;" id="signup">
                            <form enctype="multipart/form-data" action="/jobspost" method="POST" role="form">
                                <div class="row">
                                    <div class="form-group formField col-md-3">
                                        <label for="email">Company:</label>
                                        <input name="company" required type="text" class="form-control" placeholder="Enter Company Name">
                                    </div>
                                    <div class="form-group formField col-md-3">
                                        <label for="email">Company Logo:</label>
                                        <input name="avatar" type="file" class="form-control" placeholder="Enter Company Logo">
                                    </div>
                                    <div class="form-group formField col-md-3">
                                        <label for="email">Job Title:</label>
                                        <input name="jobtitle" required type="text" class="form-control" placeholder="Enter Job Title">
                                    </div>
                                    <div class="form-group formField col-md-3">
                                        <label for="email">Employement Type:</label>
                                        <select required name="employmenttype" class="form-control employmenttype" id="">
                                            <option value="Full-time">Full-time</option>
                                            <option value="Part-time">Part-time</option>
                                            <option value="Contract">Contract</option>
                                            <option value="Temporary">Temporary</option>
                                            <option value="Volunteer">Volunteer</option>
                                            <option value="Internship">Internship</option>
                                        </select>
                                    </div>
                                  </div>
                                  <div class="row">
                                    <div class="form-group formField col-md-3">
                                        <label for="email">Work from  home?</label>
                                        <div class="row justify-content-center">
                                            <div class="form-check col-4">
                                                <label class="form-check-label">
                                                  <input type="radio" class="form-check-input remote_yes" value="yes" name="remote">Yes
                                                </label>
                                              </div>
                                              <div class="form-check col-4">
                                                <label class="form-check-label">
                                                  <input type="radio" class="form-check-input remote_no" value="no" name="remote">No
                                                </label>
                                              </div>
                                        </div>    
                                    </div>
                                    <div class="form-group formField col-md-3 location_dropdowns">
                                        <label for="email">State:</label>
                                        <!-- <select name="state" class="form-control" id="stateId">
                                            <option value="">Select State</option>
                                        </select> -->
                                        <select onchange="print_city('state', this.selectedIndex);" id="sts" name ="state" class="form-control state"></select>

                                    </div>
                                    <div class="form-group formField col-md-3 location_dropdowns">
                                        <label for="email">City:</label>
                                        
                                        <select id=state name="city" class="form-control city" id="">
                                            <option value="--">--</option>
                                        </select>

                                        <!-- <select name="city" class="form-control" id="cityId">
                                            <option value="">Select City</option>
                                        </select>       -->
                                    </div>
                                    <div class="form-group formField col-md-3">
                                        <label for="email">Seniority level:</label>
                                        <select required name="senioritylevel" class="form-control senioritylevel" id="">
                                            <option value="Internship">Internship</option>
                                            <option value="Entry-level">Entry-level</option>
                                            <option value="Mid-level">Mid-level</option>
                                            <option value="Senior-level">Senior-level</option>
                                        </select>
                                    </div>
                                  </div>
                               
                                <div class="form-group formField">
                                    <label for="email">Job Description:</label>
                                    <textarea required name="jobdescription" placeholder="Enter Job Description" class="form-control" id="summernote" cols="30" rows="10"></textarea>
                                </div>
                                <div class="form-group formField">
                                    <label for="email">Add skill keywords to make your job more visible to the right candidates:</label>
                                    <select required name="skillkeywords" data-style="btn-primary" class="selectpicker" multiple data-actions-box="true" data-live-search="true">
                                        <option> Search Engine Optimisation (SEO)</option>
                                        <option> Search Engine Marketing (SEM)</option>
                                        <option> Online Advertising</option>
                                        <option> Media Sales</option>
                                        <option> Analytics</option>
                                        <option> Digital Marketing</option>
                                        <option> Programmatic Advertising</option>
                                        <option> Social Media Marketing (SMM)</option>
                                        <option> Facebook Marketing</option>
                                        <option> LinkedIN Marketing</option>
                                        <option> Twitter Marketing</option>
                                        <option> Google Ads</option>
                                        <option> Content Marketing</option>
                                        <option> Content Writing</option>
                                        <option> Inbound Marketing</option>
                                        <option> Account Management</option>
                                        <option> Media Planning</option>
                                        <option> Media Relations</option>
                                        <option> Online PR</option>
                                        <option> Facebook Ads</option>
                                        <option> Facebook Ads Manager</option>
                                        <option> Google Tag Manager (GTM)</option>
                                        <option> Facebook Business Manager</option>
                                        <option> Facebook Pixel</option>
                                        <option> Google Webmaster Tools</option>
                                        <option> Web Development</option>
                                        <option> Graphic Design</option>
                                        <option> Video Editing and creation</option>
                                        <option> Video Marketing</option>
                                        <option> Mobile Marketing</option>
                                        <option> Mobile App Marketing</option>
                                        <option> Mobile App Development</option>
                                        <option> Content Management Systems (CMS)</option>
                                        <option> Mobile Gaming</option>
                                        <option> Google Cloud Platform</option>
                                        <option> Adobe Creative Cloud</option>
                                        <option> Artificial Intelligence</option>
                                        <option> Machine Learning</option>
                                        <option> SQL </option>
                                        <option> Database Mangement</option>
                                        <option>Other</option>
                                      </select>
                                                               
                                </div>
                                <div class="form-group formField">
                                    <label for="email">How would you like to receive your applicants?</label>
                                    <div class="form-check">
                                        <label class="form-check-label">
                                          <input type="radio" class="form-check-input email_radio" value="mail" name="optradio">Notify me by email
                                          <input type="text" class="form-control" placeholder="" value="<%=typeof email!=='undefined' ? email: ''%>">
                                        </label>
                                      </div>
                                      <div class="form-check">
                                        <label class="form-check-label">
                                          <input type="radio" class="form-check-input website_radio" value="website" name="optradio">Direct applicants to an external site to apply
                                          <input name="recruiterwebsite" type="text" class="form-control" placeholder="https://www.yourcompany.com/jobs/1235">
                                        </label>
                                      </div>    
                                </div>
                                <div class="form-group formField">
                                    <%if(authenticated == true){%>
                                        <input type="submit" class="btn btn-primary btn-block bg-color-3 border-color-3" value="Post Job">
                                    <%}else{%>
                                        <input class="btn btn-primary btn-block bg-color-3 border-color-3 joblogin" value="Post Job">
                                    <%}%>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
    
            </div>
        </div>
    </section>
