      <div id="page-wrapper">
        <div class="row">
          <div  class="col-lg-12">
            <h1>Manage events <small>Create Events, Monitor Event joinees</small></h1>
            <ol class="breadcrumb">
              <li class="active"><i class="fa fa-dashboard"></i> Dashboard</li>
            </ol>
          </div>
        </div>
        <div class="">
          <ul class="nav nav-tabs">
            <li class="active"><a data-toggle="tab" href="#home">Events/Create Event</a></li>
            <li><a data-toggle="tab" href="#menu1">Event Joinees</a></li>
          </ul>

          <div class="tab-content">
            <div id="home" class="tab-pane fade in active">
              <div class="row pull-right">
                <button style="margin-bottom: 5%!important;" class="btn btn-lg btn-primary" data-toggle="modal" data-target="#myModal">Create Event</button>
              </div>
              <div style="margin-top:5%;" class="row">
                <table>
                  <thead>
                  <tr>
                    <th>Name</th>
                    <th>Description</th>
                    <th>Date</th>
                    <th>Location</th>
                    <th>Hosted by</th>
                    <th>Latitude</th>
                    <th>Longitude</th>
                  </tr>
                  </thead>
                  <tbody>
                  <%for(var i =0; i < docs.length; i++){%>
                  <tr>
                    <td><%=docs[i]['event_name']%></td>
                    <td><%=docs[i]['event_description']%></td>
                    <td><%=docs[i]['event_date']%></td>
                    <td><%=docs[i]['event_location']%></td>
                    <td><%=docs[i]['event_hostedby']%></td>
                    <td><%=docs[i]['event_latitude']%></td>
                    <td><%=docs[i]['event_longitude']%></td>
                  </tr>
                  <%}%>
                  </tbody>
                </table>
              </div>
            </div>
            <div id="menu1" class="tab-pane fade">
              <table>
                <thead>
                <tr>
                  <th>Name</th>
                  <th>Email</th>
                  <th>Phone</th>
                  <th>Message</th>
                  <th>Event Name</th>
                </tr>
                </thead>
                <tbody>
                <%for(var i =0; i < eventjoinees.length; i++){%>
                <tr>
                  <td><%=eventjoinees[i]['name']%></td>
                  <td><%=eventjoinees[i]['email']%></td>
                  <td><%=eventjoinees[i]['phone']%></td>
                  <td><%=eventjoinees[i]['message']%></td>
                  <td><%=eventjoinees[i]['event'][0].event_name%></td>
                </tr>
                <%}%>
                </tbody>
              </table>
