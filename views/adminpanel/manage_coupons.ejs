                <button style="margin-bottom: 5%!important;" class="btn btn-sm btn-primary" data-toggle="modal" data-target="#myModal">Create Coupon Code</button>
              </div>
              <div class="row">
                <table class="cell-border compact stripe hover row-border">
                  <thead>
                  <tr>
                    <th>Name</th>
                    <th>Type</th>
                    <th>Discount</th>
                    <th>Valid From</th>
                    <th>Valid To</th>
                  </tr>
                  </thead>
                  <tbody>
                  <%for(var i =0; i < docs.length; i++){%>
                  <tr>
                    <td>
                      <%=docs[i]['name']%>
                    </td>
                    <td>
                      <%=docs[i]['type']%>
                    </td>
                    <td>
                      <%=docs[i]['discount']%>
                    </td>
                    <td><%=moment(docs[i]["validfrom"]).format("DD/MMM/YYYY HH:mm A")%></td>
                    <td><%=moment(docs[i]["validto"]).format("DD/MMM/YYYY HH:mm A")%></td>
                  </tr>
                  <%}%>
                  </tbody>
                </table>
              </div>
