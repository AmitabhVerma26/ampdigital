  <div id="myModal" class="modal fade" role="dialog">
    <div class="modal-dialog">

      <!-- Modal content-->
      <div class="modal-content">
        <div style="background: lightsteelblue" class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Create new coupon code</h4>
        </div>
        <div class="modal-body">
          <div class="">
            <div class="homeContactContent">
              <form action="/courses/coupons/manage/createcouponcode" method="POST" role="form">
                <div class="form-group">
                  <label for="usr">Coupon Code:</label>
                  <input name="name" type="text" class="form-control" id="usr">
                </div>
                <div class="form-group">
                  <label for="sel1">Select Discount Type:</label>
                  <select name="type" class="form-control" id="sel1">
                    <option value="amount">Amount</option>
                    <option value="percentage">Percentage</option>
                  </select>
                </div>
                <div class="form-group">
                  <label for="discount">Discount:</label>
                  <input name="discount" type="number" class="form-control" id="discount">
                </div>
                <div class="form-group">
                  <label for="validfrom">Valid From:</label>
                  <input type="date" name="validfrom" class="form-control" id="validfrom">
                </div>
                <div class="form-group">
                  <label for="validto">Valid To:</label>
                  <input type="date" name="validto" class="form-control" id="validto">
                </div>
                <button type="submit" class="btn btn-primary btn-sm">Create Coupon Code</button>
              </form>
            </div>
          </div>
        </div>
        <div style="border: 0;" class="modal-footer">
        </div>
      </div>

    </div>
