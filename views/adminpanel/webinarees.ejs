    <script type="text/javascript">
      $(document).ready(function () {
        $('.applyfilter').on('click', function () {
          table.ajax.reload(null, false);
        })

        $('.resetfilter').on('click', function () {
          $('#purposefilter').val('_');
          $('#fromdatefilter').val('');
          $('#todatefilter').val('');
          table.ajax.reload(null, false);
        })

        let table = $('#table').DataTable({
          "bProcessing": true,
          "bServerSide": true,
          "bInfo": true,
          "sAjaxSource": "/datatable/webinarattendees",
          "fnServerParams": function (aoData) { },
          dom: 'Bfrtip',
          buttons: [
            'pageLength', 'excel', 'print'
          ],
          lengthMenu: [
            [10, 25, 50, -1],
            ['10 rows', '25 rows', '50 rows', 'Show all']
          ],
          "language":
          {
            "processing": "<i class='fa fa-spinner fa-spin' style='font-size:24px'></i>",
          },
          "drawCallback": function (nRow, aData, iDisplayIndex, iDisplayIndexFull) {
            $('.js-example-basic-multiple').select2();
            $('.addcertificate').on('submit', function (e) {
              e.preventDefault();
              var id = $(this).data('id');
              var webinarid = $(this).data('webinarid');
              var email = $(this).data('email');
              var name = $(this).data('name');
              var selected = $(this).children('select').val();
              var certificates = ($(this).data("certificates"));
              var arr = certificates.split(",");
              if (arr && typeof arr !== "undefined" && arr.length > 1) {
                certificates = arr;
              }
              else if (certificates.length > 0) {
                certificates = [certificates];
              }
              else {
                certificates = [];
              }
              var notcertify = selected.every((el) => {
                return certificates.indexOf(el) !== -1;
              });
              var certify = !notcertify;
              $(this).data("certificates", selected.join(","))
              if (certify) {
                if (selected == null) {
                  $.ajax({
                    type: "PUT",
                    url: '/updatecertificatewebinar',
                    data: {
                      'id': id,
                      'webinarid': webinarid,
                      'courses': '',
                      'email': email,
                      'name': name,
                      'length': 0
                    },
                    success: function (result) {
                      if (result == 1) {
                        $.alert('Access updated successfully!')
                      }
                      else {
                        $.alert('An error occurred!');
                      }
                    }
                  });
                }
                else {
                  var length = selected.length;
                  selected = selected.join(',');
                  $.ajax({
                    type: "PUT",
                    url: '/updatecertificatewebinar',
                    data: {
                      'id': id,
                      'webinarid': webinarid,
                      'email': email,
                      'name': name,
                      'courses': selected,
                      'length': length
                    },
                    success: function (result) {
                      if (result == 1) {
                        $.alert('Certificate provided successfully!')
                      }
                      else {
                        $.alert('An error occurred!');
                      }
                    }
                  });
                }
              }
              else {
                $.alert("Already certified for these courses");
              }


            });
          }
        });
      });
    </script>
