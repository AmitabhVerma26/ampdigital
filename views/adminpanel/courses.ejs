    <div class="">

      <div class="tab-content">
        <div id="home" class="tab-pane fade in active">
          <div class="row">
            <button style="background-color: #2972d1!important; border-color: #2972d1!important;" class="btn btn-primary btn-sm pull-right" data-toggle="modal" data-target="#addNewCourseModal">Add New Course</button>
            <br><br><br>
            <table class="cell-border compact stripe hover row-border">
              <thead>
              <tr>
                <th>Course Name</th>
                <th>Image</th>
              <th>Upload/Edit Image</th>
                <th>View Details</th>
                <th>Course Price</th>
                <th>Course Tax (in %)</th>
                <th>Course Discount (in %)</th>
                <th>Discount till</th>
                <th>Discount Active?</th>
                <th>Objective</th>
                <th>Duration</th>
                <th>Course Modules</th>
                <th>Course Level</th>
                <th>Course Created on</th>
                <th>Action</th>
                <th>Course URL</th>
                <th>Course Live?</th>
              </tr>
              </thead>
              <tbody>
              <%for(var i = 0; i < docs.length; i++){%>
              <tr>
                <td>
                  <a class="updatecoursename approtitle" id="course_name" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/courses/updateinfo" data-title="Enter title"><%= docs[i]['course_name'] %></a>
                </td>
                <td>
                  <%if(docs[i]['course_image'] && docs[i]['course_image'].split('?')){%>
                <a href="<%=docs[i]['course_image'].split('?')[0]%>"><span class="label label-success"><i>View Image</i></span><i class="fa fa-image"></i></a>
                  <%}else{%>
                <span class="label label-info"><i>No Image Uploaded</i></span>
                  <%}%>
              </td>
              <td>
                <!--<a class="updatemodule approtitle" id="course_name" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/updatemoduleimage" data-title="Enter image"><%= docs[i]['module_image'] %></a>-->
                <form enctype="multipart/form-data" class="imagesubmitform" action="/courses/uploadimage" method="POST" target="_blank">
                  <label>
                    <input name="courseid"  type="hidden" value="<%=docs[i]['_id']%>">
                    Browse <input name="avatar" class="imagetosubmit" type="file" hidden>
                  </label>
                  <button class="btn btn-xs btn-primary imagesubmitformbtn" type="submit">Submit</button>
                </form>
              </td>
                <td><button data-courseid="<%=docs[i]['_id']%>" class="btn btn-primary btn-xs course_modules">Course Modules</button><br> <br>
                  <button data-courseid="<%=docs[i]['_id']%>" class="btn btn-info btn-xs course_faqs">Course FAQs</button><br><br>
                  <button data-courseid="<%=docs[i]['_id']%>" class="btn btn-info btn-xs coures_features">Course Features</button><br><br>
                </td>
                <td>
                  <a class="updatecourseprice approtitle" id="course_price" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/courses/updateinfo" data-title="Enter price"><%= docs[i]['course_price'] %></a>
                </td>
                <td>
                  <a class="updatecoursetax approtitle" id="course_tax" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/courses/updateinfo" data-title="Enter tax"><%= docs[i]['course_tax'] %></a>
                </td>
                <td>
                  <a class="updatecoursediscount approtitle" id="course_discount" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/courses/updateinfo" data-title="Enter discount"><%= docs[i]['course_discount'] %></a>
                </td>
                <td>
                  <a class="updatecoursediscountvalid approtitle" id="course_discount_valid" data-type="date" data-pk="<%= docs[i]['_id'] %>" data-value="<%=moment(docs[i]['course_discount_valid']).format( 'YYYY-MM-DD') %>" data-url="/courses/updateinfo" data-title="Discount valid till"><%=moment(docs[i]['course_discount_valid']).format( 'YYYY-MMMM-DD') %></a>
                </td>
                <td>
                  <%=docs[i]['course_discountlive']%>
                  <a class="updatecoursediscountactive approtitle" id="course_discountactive" data-type="select" data-pk="<%= docs[i]['_id'] %>" data-url="/courses/updateinfo" data-title="Discount Active?"><%= docs[i]['course_discountlive'] %></a>
                </td>
                <td>
                  <a class="updatecoursename approtitle" id="course_objective" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/courses/updateinfo" data-title="Enter title"><%=docs[i]['course_objective']%></a>
                </td>
                <td>
                  <a class="updatecoursename approtitle" id="course_duration" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/courses/updateinfo" data-title="Enter title"><%=docs[i]['course_duration']%></a>
                </td>
                <td>
                  <a class="updatecoursename approtitle" id="course_modules" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/courses/updateinfo" data-title="Enter course modules"><%=docs[i]['course_modules']%></a>
                </td>
                <td>
                  <a class="updatecoursename approtitle" id="course_target_audience" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/courses/updateinfo" data-title="Enter title"><%=docs[i]['course_target_audience']%></a>
                </td>
                <td><%=moment(docs[i]['course_createdon']).format( 'MMM-DD-YYYY')%></td>
                <td><a class="removecourse" data-course_id="<%=docs[i]['_id']%>" href=""><i class="fa fa-trash-o"></i>Remove</a></td>
                <td>
                  <a class="updatecoursename approtitle" id="course_url" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/courses/updateinfo" data-title="Enter title"><%= docs[i]['course_url'] %></a>
                </td>
                <td>
                  <a class="updatecourselive approtitle" id="course_live" data-type="select" data-pk="<%= docs[i]['_id'] %>" data-url="/courses/updateinfo" data-title="Course Live?"><%= docs[i]['course_live'] %></a>
                </td>
              </tr>
              <%}%>
              </tbody>
            </table>
          </div>
        </div>
