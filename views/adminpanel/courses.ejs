            });

            $('.updatecourselive').editable({
                value: '',    
                source: [
                {value: '', text: '--'},
                      {value: 'Live', text: 'Live'},
                      {value: 'Not Live', text: 'Not Live'}
                  ]
            });

            $('.updatecoursediscountvalid').editable({
              format: 'yyyy-mm-dd',
    viewformat: 'yyyy-mm-dd',   
        datepicker: {
                weekStart: 1
           }
        });

            $('.updatecoursediscountactive').editable({
                value: 'Not Active',    
                source: [
                {value: "Not Active", text: '--'},
                      {value: "Active", text: 'Active'},
                      {value: "Not Active", text: 'Not Active'}
                  ]
            });
                $('.updatecoursename').editable({tpl: '<textarea maxlength="10000"></textarea>', success: function(response, newValue) {
                    }});
            }
        });

        //Handler to add a new course
        $('#addnewcourse').on('click', function () {
            var course_name = $('#newcourseinput').val();
            var course_objective = $('#newcourseobjective').val();
            var course_projects = $('#newcourseprojects').val();
            var course_duration = $('#newcourseduration').val();
            var course_target_audience = $('#newcoursetargetaudience').val();
            var course_tools_requirements = $('#newcourserequirements').val();
            $.ajax({
                type: "POST",
                url: '/courses/addcourse',
                data: {
                    course_name: course_name,
                    course_objective: course_objective,
                    course_projects: course_projects,
                    course_duration: course_duration,
                    course_target_audience: course_target_audience,
                    course_tools_requirements: course_tools_requirements
                },
                success: function(result){
                    $.alert('Course Added Successfully!');
                    location.reload();
                }
            });
        });

        //Handler to navigate to the course's modules
        $('.course_modules').on('click', function () {
            var course_id = $(this).data('courseid');
            window.location = '/courses/'+course_id+'/modules/manage';
        });

        $('.course_faqs').on('click', function () {
            var course_id = $(this).data('courseid');
            window.location = '/courses/faqs/manage/'+course_id;
        });

        $('.course_features').on('click', function () {
            var course_id = $(this).data('courseid');
            window.location = '/addfeature/'+course_id;
        });

        $('.removecourse').on('click', function (e) {
            e.preventDefault();
            var course_id = $(this).data('course_id');
            $.confirm({
                title: 'Confirm!',
                content: 'Are you sure you want to delete the course?',
                buttons: {
                    confirm: function () {
                        $.ajax({
                            type: "PUT",
                            url: '/removecourse',
                            data: {
                                course_id: course_id
                            },
                            success: function(result){
                                $.alert('Course Removed Successfully!');
                                location.reload();
                            }
                        });
                    },
                    cancel: function () {

                    },
                }
            });
        });
    });
</script>
</body>
</html>
