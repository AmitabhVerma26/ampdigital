          success: function(response, newValue) {
            location.reload();
              // alert('hello');
          },
          display: function(value) {
          }
      });
     
      $(this).scrollTop(0);
      window.onbeforeunload = function () {
          window.scrollTo(0, 0);
      }
      $('.removefaq').on('click', function (e) {
          e.preventDefault();
          var faqid = $(this).data('pk');
          $.confirm({
              title: 'Prompt!',
              content: 'Are you sure to delete the faq?',
              buttons: {
                  Yes: {
                      text: 'Yes',
                      btnClass: 'btn-blue',
                      action: function () {
                          $.ajax({
                              type: "DELETE",
                              url: '/courses/faqs/removefaq',
                              data: {
                                  faqid: faqid
                              },
                              success: function(result){
                                  $.alert('FAQ Deleted!');
                                  location.reload();
                              }
                          });
                      }
                  },
                  cancel: function () {
                      //close
                  },
              }
          });
      });
      $('table').DataTable({
          "initComplete": function(settings, json) {
          }
      });
    });
</script>
</body>
</html>
