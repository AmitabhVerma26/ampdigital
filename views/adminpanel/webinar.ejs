                    <div class="form-group col-md-12">
                        <label for="example-text-input" class="col-form-label pull-left">Name of Webinar</label>
                        <input name="webinarname" class="form-control" type="text" id="example-email-input">
                    </div>
                    <div class="form-group col-md-12">
                        <label for="example-text-input" class="col-form-label pull-left">Duration (in mins)</label>
                        <input name="duration" class="form-control" type="number" id="example-email-input">
                    </div>
                    <div class="form-group col-md-12">
                      <label for="example-text-input" class="col-form-label pull-left">Date</label>
                      <input name="date" class="form-control" type="datetime-local" id="example-email-input">
                  </div>
                  <div class="form-group col-md-12">
                    <label for="example-text-input" class="col-form-label pull-left">Name of Speaker</label>
                    <input name="speakername" class="form-control" type="text" id="example-email-input">
                </div>

                <div class="form-group col-md-12">
                  <label for="example-text-input" class="col-form-label pull-left">Speaker Description</label>
                  <textarea rows="3" name="speakerdescription" class="form-control" type="text" id="example-text-input"></textarea>
              </div>
              <div class="form-group col-md-12">
                <label for="example-text-input" class="col-form-label pull-left">Level</label>
                <input name="level" class="form-control" type="text" id="example-email-input">
            </div>
