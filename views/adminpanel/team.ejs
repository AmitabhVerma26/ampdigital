          <div class="row">
            <button class="btn btn-primary btn-sm pull-right" data-toggle="modal" data-target="#addNewCourseModal">Add New Team Member</button>
            <br><br>
            <table class="cell-border compact stripe hover row-border">
              <thead>
              <tr>
                <th>Name</th>
                <th>Designation</th>
                <th>Qualification</th>
                <th>LinkedIn profile URL</th>
                <th>Image</th>
                <th>Upload/Update Image</th>
                <th>Action</th>
              </tr>
              </thead>
              <tbody>
              <%for(var i = 0; i < docs.length; i++){%>
              <tr>
                <td>
                  <a class="updateteam" id="name" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/updateteam" data-title="Enter name"><%= docs[i]['name'] %></a>
                </td>
                <td>
                  <a class="updateteam" id="designation" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/updateteam" data-title="Enter designation"><%= docs[i]['designation'] %></a>
                </td>
                <td>
                  <a class="updateteam" id="qualification" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/updateteam" data-title="Enter qualification"><%= docs[i]['qualification'] %></a>
                </td>   
                <td>
                  <a class="updateteam" id="linkedinprofileurl" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/updateteam" data-title="Enter linkedin profile url"><%= docs[i]['linkedinprofileurl'] %></a>
                </td>                
                <td>
                  <%if(docs[i]['imageurl'] && docs[i]['imageurl'].split('?')){%>
                <a href="<%=docs[i]['imageurl'].split('?')[0]%>"><i class="fa fa-image"></i>View</a>
                  <%}else{%>
                <span class="label label-info"><i>No Image Uploaded</i></span>
                  <%}%>
              </td>
              <td>
                <!--<a class="updatemodule approtitle" id="course_name" data-type="textarea" data-pk="<%= docs[i]['_id'] %>" data-url="/updatemoduleimage" data-title="Enter image"><%= docs[i]['module_image'] %></a>-->
                <form enctype="multipart/form-data" class="imagesubmitform" action="/updateteampersonpicture" method="POST">
                  <label>
                    <input name="id"  type="hidden" value="<%=docs[i]['_id']%>">
                    Browse <input name="avatar" class="imagetosubmit" type="file" hidden>
                  </label>
                  <button class="btn btn-xs btn-primary imagesubmitformbtn" type="submit">Submit</button>
                </form>
              </td>
                <td><a class="removeteamperson" data-id="<%=docs[i]['_id']%>" href=""><i class="fa fa-trash-o"></i>Remove</a></td>
              </tr>
              <%}%>
              </tbody>
            </table>
          </div>
