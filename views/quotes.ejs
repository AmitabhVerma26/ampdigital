<body>
  <!-- Modal -->
  <div id="myModal" class="modal fade" role="dialog">
    <div class="modal-dialog">

      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Create new event</h4>
        </div>
        <div class="modal-body">
          <div class="">
            <div class="homeContactContent">
              <form action="createevent" method="POST" role="form">
                <div class="form-group">
                  <i class="fa fa-user"></i>
                  <input name="event_name" required type="text" class="form-control border-color-1" id="event_name"
                    placeholder="Event name">
                </div>
                <div class="form-group">
                  <i class="fa fa-file-text" aria-hidden="true"></i>
                  <textarea name="event_description" required class="form-control border-color-2"
                    placeholder="Event Description"></textarea>
                </div>
                <div class="form-group">
                  <div class='input-group date' id='datetimepicker1'>
                    <input name="event_date" type='text' class="form-control" />
                    <span class="input-group-addon">
                      <span class="fa fa-calendar"></span>
                    </span>
                  </div>
                </div>
                <div class="form-group">
                  <i class="fa fa-location-arrow"></i>
                  <input name="event_location" required type="text" class="form-control border-color-6"
                    id="event_location" placeholder="Event location">
                </div>
                <div class="form-group">
                  <i class="fa fa-user"></i>
                  <input name="event_hostedby" required type="text" class="form-control border-color-4"
                    id="event_hostedby" placeholder="Event hosted by">
                </div>
                <div class="form-group">
                  <i class="fa fa-globe"></i>
                  <input name="event_latitude" required type="text" class="form-control border-color-5"
                    id="event_latitude" placeholder="Event latitude">
                </div>
                <div class="form-group">
                  <i class="fa fa-globe"></i>
                  <input name="event_longitude" required type="text" class="form-control border-color-6"
                    id="event_longitude" placeholder="Event longitude">
                </div>
                <button type="submit" class="btn btn-primary">Create Event</button>
              </form>
            </div>
          </div>
        </div>
        <div style="border: 0;" class="modal-footer">
        </div>
      </div>

    </div>
  </div>

  <nav class="navbar navbar-fixed-top fixed-top navbar-expand-md bg-light navbar-default mb-1" style="background: white!important; padding-bottom: 3%;">
