<!-- header-->
<%-include partials/header.ejs%>

    <div class="container">
      <div class="row">
      
        <%for(let i = 0; i< courses.length; i++){
          let course = courses[i];
          let today = new Date();
          today.setHours(0,0,0,0);
          let discountvaliddate = new Date(course.course_discount_valid);
          discountvaliddate.setHours(0,0,0,0);
          let course_price = ((1+0.01*parseInt(course.course_tax))*parseInt(course.course_price)).toString()
          course_price = Math.floor(course_price);
          let discounted_price = (course.course_discountactive=="Active" &&  (discountvaliddate == today || discountvaliddate > today)) ? course_price*(100-course.course_discount)*.01 :course_price
          discounted_price = Math.floor(discounted_price); 
          let validtill = (course.course_discountactive=="Active" &&  (discountvaliddate == today || discountvaliddate > today)) ? "(till "+moment(course['course_discount_valid']).format( 'DD MMMM') + ")" :"";
          
          %>
    <div class="col-md-4 d-flex align-items-stretch">
      <div data-href="/courses/<%=courses[i]['course_url']%>" class="course" style="cursor: pointer;">
        <div class="course-image" style="height:150px!important;">
          <a href="/courses/<%=courses[i]['course_url']%>">
            <img class="lazy" width="100%" src="<%=courses[i]['course_image']%>" alt="<%=courses[i]['course_name']%>">
          </a>
          <div class="overlay flex-column d-flex align-items-center justify-content-center">
           
          </div>
        </div>
        <div class="course-footer d-flex align-items-center justify-content-between">
          <!-- <div class="comments"><i class="icon-chat"></i><span>340</span></div> -->
          <div class="learnmore"><a href="/courses/accomplishments/<%=certificateuser._id%>/<%=courses[i]['course_url']%>" class="btn btn-primary">View Certificate</a>
          </div>
        </div>
      </div>
    </div>
    <%}%>
