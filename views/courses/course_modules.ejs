<!-- LOGIN MODAL -->
<!-- Modal -->
<div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header bg-primary">
                <h5 class="modal-title color-white" id="exampleModalLabel">Login</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="text-center forgotpasswordinstructionssent d-none">
                    <i style="font-size: 75px;" class="fa fa-plane color-4"></i>
                    <h3>
                        Instructions on resetting your password are on their way!
                    </h3>
                    <p>If you don't receive an email soon ensure that the email is valid and that you can receive emails
                        from amitabh@ampdigital.co. Contact amitabh@ampdigital.co for additional assistance.</p>
                </div>
                <form class="d-none forgotpasswordform" action="/forgotpassword" method="POST" role="form">
                    <div class="form-group formField">
                        <input name="email" required type="email" class="form-control border-color-1" id="forgotpasswordemail" placeholder="Email">
                    </div>
                    <button type="submit" class="btn btn-primary">Forgot Password</button>
                    <div class="form-group formField">
                        <p class="help-block">Not What you are looking for?<a class="loginformbutton" href="#">Login</a> instead</p>
                    </div>
                </form>
                <form class="loginform" action="/login" method="POST" role="form">
                    <div class="form-group formField">
                        <input name="email" required type="text" class="form-control border-color-1" id="email" placeholder="Email">
                    </div>
                    <div class="form-group formField">
                        <input name="password" required type="password" class="form-control border-color-2 forgotpasswordbtn" id="password" placeholder="Password">
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                    <div class="form-group formField">
                        <p class="help-block"><a class="forgotpasswordbutton" href="#">Forgot password?</a></p>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<!-- CREATE ACCOUNT MODAL -->
<div class="modal fade" id="createAccount" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
