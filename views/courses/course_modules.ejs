    $(document).ready(function () {
        $(".viewmodulebtn").removeClass("disabled");
        $(".viewmodulebtn, .module-title").on("click", function(e){
            e.preventDefault();
            if($(this).data("active")=="Not Active"){
                alert("This module will open on "+moment($(this).data("opens_on")).format("DD/MMM/YYYY"))
            }
            else{
                window.location.href = $(this).data("href");
            }
        })
        $('.backbutton').on('click', function (e) {
            e.preventDefault();
            window.history.back();
        })

        $('.forgotpasswordbutton').on('click', function (e) {
            e.preventDefault();
            $('.loginform').addClass('d-none');
            $('.forgotpasswordform').removeClass('d-none');
            $('.loginpaneltitle').html('Forgot Password');
        });

        $('.loginformbutton').on('click', function (e) {
            e.preventDefault();
            $('.loginform').removeClass('d-none');
            $('.forgotpasswordform').addClass('d-none');
            $('.loginpaneltitle').html('Login');
        });

        $('.forgotpasswordform').on('submit', function (e) {
            $(this).children('button').attr('disabled', 'disabled');
            e.preventDefault();
            var email = $('#forgotpasswordemail').val();
            $.ajax({
                method: "POST",
                url: "/forgotpassword",
                data: {email: email}
            }).done(function(response) {
                if(response._id){
                    $('#loginModal').modal('toggle');
                    $.alert('<div class="text-center"> <i style="font-size: 75px;" class="fa fa-plane color-primary"></i>\n' +
                        '            <h3>\n' +
                        '              Instructions on resetting your password are on their way!\n' +
                        '            </h3>\n' +
                        '            <p>If you do not receive an email soon ensure that the email is valid and that you can receive emails\n' +
                        '              from amitabh@ampdigital.co. Contact amitabh@ampdigital.co for additional assistance.</p></div>');              }
                console.log(response);
            });
        });
        $('.slbutton').on('click', function () {
            var index;  //Index of last watched if not 0;
            var percentagewatchedArray = [];
            $('.pc').each(function () {
                percentagewatchedArray.push($(this).data('percentagecompleted'));
            });
            // alert(percentagewatchedArray);
            var watched = -1; //Tracking last watched initializing it to first module
            for(var j = percentagewatchedArray.length-1; j > -1; j--){
                if(percentagewatchedArray[j]!==0){
                    watched = 1;
                    index = j;
                    break;
                }
            }

            if(watched == -1){
                var cnt = 0;
                $('.vmblg').each(function () {
                    if(cnt == 0){
                        window.location.href = $(this).attr('href');
                    }
                    cnt++;
                })
            }
            else{
                var cnt = 0;
                $('.vmblg').each(function () {
                    if(cnt == index){
                        window.location.href = $(this).attr('href');
                    }
                    cnt++;
                })

            }
        })
    })
</script>
