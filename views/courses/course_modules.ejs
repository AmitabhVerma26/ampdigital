      <script type="text/javascript">
      $(document).ready(function(){
          var courseprogress = [];
          let currentModuleIndex = 0;
          $(".courseprogress").each(function(){
              courseprogress.push(parseInt($(this).data('courseprogress')));
          })
          for(var i = courseprogress.length-1; i >0; i--){
              if(courseprogress[i]==0){
                currentModuleIndex = i-1
              }
              else{
                  break;
              }
          }
          $('.slbutton').on('click', function(){
              var moduleurl = $('.module'+currentModuleIndex).data('href');
              window.location.href = moduleurl;
          })
         setTimeout(function(){
              $.getScript("https://sdk.amazonaws.com/js/aws-sdk-2.41.0.min.js");
              $.getScript("https://www.ampdigital.co/js/chatbot.js");
            }, 2000);
      })
