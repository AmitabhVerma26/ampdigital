    <div class="container">
      <div class="row justify-content-center">
        <div class="col-md-8">
          <div id="accordion">
            <% for(var i = 0; i<faqdocs.length; i++){ %>
            <div class="card">
              <div class="card-header" id="heading-<%=i%>">
                <h5 class="mb-0">
                  <a role="button" data-toggle="collapse" href="#faq-collapse-<%=i%>" aria-expanded="<%= i==0? 'true': 'false' %>" aria-controls="faq-collapse-1">
                    <%=faqdocs[i]['_id'].category%>
                  </a>
                </h5>
              </div>
              <div id="faq-collapse-<%=i%>" class="collapse <%= i==0? 'show': '' %>" data-parent="#accordion" aria-labelledby="faq-heading-<%=i%>">
                <div class="card-body">
          
                  <div id="faq-accordion-<%=i%>">
                    <% for(var j = 0; j < faqdocs[i].question.length; j++){%>

                    <div class="card">
                      <div class="card-header" id="faq-heading-<%=i%>-<%=j%>">
                        <h5 class="mb-0">
                          <a class="collapsed" role="button" data-toggle="collapse" href="#faq-collapse-<%=i%>-<%=j%>" aria-expanded="false" aria-controls="faq-collapse-<%=i%>-<%=j%>">
                            Q. <%=faqdocs[i].question[j]%>
                          </a>
                        </h5>
                      </div>
                      <div id="faq-collapse-<%=i%>-<%=j%>" class="collapse <%= j==0? 'show': '' %>" data-parent="#faq-accordion-1" aria-labelledby="faq-heading-<%=i%>-<%=j%>">
                        <div class="card-body">
                          <p class="mb-0">
                            <%-faqdocs[i].answer[j]%>
                          </p>
                        </div>
                      </div>
                    </div>
                    <%}%>
                  </div>      
                
                </div>
              </div>
            </div>
            <%}%>
          </div>
        </div>
      </div>
  </div>
