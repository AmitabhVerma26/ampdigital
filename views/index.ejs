  background: #fafafa;
  border: 1px solid #e7e7e7;
  margin-top: 30px;
  position: relative;
  border-radius: 30px;
}

footer.bg-dark .item .subscribe form {
  background: rgba(255, 255, 255, 0.06);
  border: 1px solid rgba(255, 255, 255, 0.1);
}

